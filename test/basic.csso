
//Import "plop.csso";

Class 	List {
	@listItemSize: 		20px;


	Function List() :
		"a" 	-> link();
	{
		display: 	block;
		float: 		left;
		width: 		100%;
		height: 	(@listItemSize);
	}

	Function a() {
		color: 		white;
	}
}


Class 	MyList Extends List {
	@lal: 			 (@plop - (2px * (3px * -1)));

	Function MyList(@plop) :
		"li" 	-> Std.li();
	{
		Super();
		border:  	1px solid #ff0000;
	}

}
